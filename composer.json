{
    "name": "dpoposki/symfony-kernel",
    "description": "A kernel bundle for the Symfony framework",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "doctrine/doctrine-bundle": "^2.12",
        "symfony/framework-bundle": "7.0.*",
        "symfony/messenger": "7.0.*",
        "symfony/validator": "7.0.*",
        "symfony/yaml": "7.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.11|^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Poposki\\KernelBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Poposki\\KernelBundle\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Darko Poposki",
            "email": "darko@poposki.mk"
        }
    ]
}
